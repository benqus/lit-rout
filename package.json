{
  "name": "lit-rout",
  "version": "0.0.5",
  "description": "Declarative, conditional rendering based on the Location for Lit applications",
  "source": "./lib/index.ts",
  "main": "./build/index.js",
  "module": "./module/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn type-check && yarn test && yarn parcel",
    "dist": "yarn build && yarn terser",
    "lint": "eslint \"./lib/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "terser:build": "terser ./build/index.js --mangle --mangle --comments false --output ./build/index.min.js",
    "terser:module": "terser ./module/index.js --mangle --mangle --comments false --output ./module/index.min.js",
    "terser": "yarn terser:build && yarn terser:module",
    "parcel": "parcel build ./lib/index.ts --dist-dir build --log-level info --no-cache --no-source-maps --detailed-report 10",
    "test": "yarn lint",
    "prepare": "yarn dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benqus/lit-rout.git"
  },
  "keywords": [
    "lit",
    "routing"
  ],
  "author": "Bence Kormos <ben.kormos@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benqus/lit-rout/issues"
  },
  "homepage": "https://github.com/benqus/lit-rout#readme",
  "dependencies": {
    "@swc/helpers": "^0.2.14",
    "lit": "^2.0.2",
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/reporter-bundle-analyzer": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "parcel": "^2.0.1",
    "prettier": "^2.4.1",
    "terser": "^5.10.0",
    "typescript": "4.3.5"
  }
}
